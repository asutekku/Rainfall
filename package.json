{
  "name": "rainfall",
  "version": "1.0.0",
  "description": "Turn based RPG game",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "lint": "tslint './src/**/*.ts*' --format stylish --force",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "yarn run build && node express.js",
    "test": "jest --watch --coverage --config=configs/jest.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asutekku/Rainfall.git"
  },
  "author": "asutekku",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/asutekku/Rainfall/issues"
  },
  "homepage": "https://github.com/asutekku/Rainfall#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "@spyna/react-store": "^1.3.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "konva": "^3.2.3",
    "node-sass": "^4.12.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.8.2",
    "react-konva": "^16.8.6",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "sass-loader": "^7.1.0",
    "scss-loader": "^0.0.1",
    "style-loader": "^0.23.1",
    "tslint": "^5.11.0",
    "two.js": "^0.7.0-beta.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@types/node": "^11.13.0",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "awesome-typescript-loader": "^5.2.1",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.2.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  }
}
